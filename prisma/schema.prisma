
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}



datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}



model User {
  id    Int     @default(autoincrement()) @id
  email String  @unique
  name  String?
  password String
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}